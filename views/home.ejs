    <script type="text/javascript">
        window.onscroll= function () {myScroll()};
        function myScroll() {
            if ($('html').scrollTop() > 0){
                $('#nav_bar').css('backgroundColor','white');
                $('.bar_item .full >a').css('color','#2F2911');
                $('#logo .full a img').attr("src", "imgs/phuong/logo.png");
            }else if($('html').scrollTop() == 0){
                $('#nav_bar').css('backgroundColor','unset');
                $('.bar_item .full >a').css('color','white');
                $('#logo .full a img').attr("src", "imgs/phuong/logo_white.png");
            }
            if ($('html').scrollTop() > 39){
                $('.home_info1').addClass('home_abus_bach');
                $('.home_info2').addClass('home_abus_phuong');
                $('.home_info3').addClass('home_abus_hoa');
            }
        }
        function bar_enter(e) {
            //$('.bar_item').removeClass('bar_enter');
            e.classList.add('bar_enter');
        }
        function bar_leave(e) {
            e.classList.remove('bar_enter');
        }

        function btn_enter(e) {
            e.classList.add('btn_enter');
        }
        function btn_leave(e) {
            e.classList.remove('btn_enter');
        }
        var cnt_time=1;

        setInterval(function () {
