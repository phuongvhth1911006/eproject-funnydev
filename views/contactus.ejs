    </section>
<%- include('footer.ejs') -%>
<script src="/jquery/jquery.js" type="text/javascript"></script>
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
<script src="/nivo-slider/jquery.nivo.slider.js" type="text/javascript"></script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCaj0mHlR-keu-9hsR6d-gB0L9BclG04rk&callback=initMap" async defer></script>
<script type="text/javascript">
    var map;
    function initMap() {
        var position = {
            lat: 11.940422,
            lng: 108.4363515
        };
        map = new google.maps.Map(document.getElementById('map'), {
            center: position,
            zoom: 16
        });
        var marker = new google.maps.Marker({
            position:position,
            map: map,
            label: {text:'Funny Group',color:"black",fontSize:"25px"}
        });
    }
</script>
</body>
</html>